<server:level name="test">
    <RoomPlacement class="Default">
        <MinimumArea>1000</MinimumArea>
        <MaximumArea>4000</MaximumArea>
        <Room class="test">
            <Frequency>1</Frequency>
        </Room>
    </RoomPlacement>
</server:level>

<server:room name="test">
    <RoomLayout class="Default">
        <MinimumWidth>8</MinimumWidth>
        <MinimumHeight>8</MinimumHeight>
        <MaximumWidth>16</MaximumWidth>
        <MaximumHeight>16</MaximumHeight>
    </RoomLayout>
    <ItemPlacement class="Default">
        <Debris>debris</Debris>
    </ItemPlacement>
</server:room>

<server:item name="debris">
    <EntitySelector class="Default">
        <Entity class="dust_0" freq="25" />
        <Entity class="dust_1" freq="25" />
        <Entity class="dust_2" freq="25" />
        <Entity class="dust_3" freq="25" />
    </EntitySelector>
</server:item>
