<server:entity name="character">
    <StaticDrawable>
        <Layer>Characters</Layer>
        <Symbol>&#0001;</Symbol>
        <ForeColor>White</ForeColor>
    </StaticDrawable>
</server:entity>

<server:entity name="player" base="character">
    <StaticDrawable>
        <Symbol>@</Symbol>
        <ForeColor>Yellow</ForeColor>
    </StaticDrawable>

    <PlayerControl>
        <MovePeriod>1</MovePeriod>
    </PlayerControl>
</server:entity>
